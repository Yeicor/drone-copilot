#: kivy 2.1.0
#: import MyVideo app.video.video
#: import DefaultTracker app.video.tracker

# This is the video player widget, including the tracker overlay.

<MyVideo>:

    DefaultTracker:
        id: tracker
        pos: self.parent.pos
        size: self.parent.size
        video: self.parent  # Use it as a child (overlay) of MyVideo
